{
  "name": "patriziotomato/laravel-jira",
  "description": "Easily access the Jira-API in your Laravel Application",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^8.2",
    "laravel/framework": "^9.0",
    "lesstif/php-JiraCloud-RESTAPI": "^1.3",
    "khill/php-duration": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "LaravelJira\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelJira\\JiraServiceProvider"
      ],
    }
  }
}
